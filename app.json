    "slug": "hello-arequipa",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.7.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
